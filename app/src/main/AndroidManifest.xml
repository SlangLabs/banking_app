<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akanshisrivastava.bankingapp">

    <application
        android:name=".SlangMockApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MoneyTransfer" />
        <activity android:name=".Accounts" />
        <activity android:name=".CustomerCare" />
        <activity android:name=".Bills" />
        <activity android:name=".Services" />
        <activity android:name=".Statement" />
        <activity android:name=".RecentTransactions" />
        <activity android:name=".slang.OrderCheque"></activity>
    </application>

</manifest>