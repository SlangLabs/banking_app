<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akanshisrivastava.bankingapp">

    <application
        android:name="in.slanglabs.bankingapp.SlangMockApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="in.slanglabs.bankingapp.MainActivity" />
        <activity android:name="in.slanglabs.bankingapp.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="in.slanglabs.bankingapp.MoneyTransfer" />
        <activity android:name="in.slanglabs.bankingapp.Accounts" />
        <activity android:name="in.slanglabs.bankingapp.CustomerCare" />
        <activity android:name="in.slanglabs.bankingapp.Bills" />
        <activity android:name="in.slanglabs.bankingapp.Services" />
        <activity android:name="in.slanglabs.bankingapp.Statement" />
        <activity android:name="in.slanglabs.bankingapp.RecentTransactions" />
        <activity android:name="in.slanglabs.bankingapp.OrderCheque" />
        <activity android:name="in.slanglabs.bankingapp.BillPayment" />
        <activity android:name="in.slanglabs.bankingapp.AccountStatement"></activity>
    </application>

</manifest>